<div class="container table-responsive">
  <table class="table table-hover clickable-table">
    <thead>
      <tr>
        <th>Name</th>
        <th>Headline</th>
      </tr>
    </thead>
    <tbody>
      <% for(i in banners ) {%>
      <tr >
        <td>
          <a href="/banner/show/<%=banners[i].id%>">
            <%=banners[i].name%>
          </a>
        </td>
        <td>
          <a href="/banner/show/<%=banners[i].id%>">            
          <%=banners[i].headline%>
          </a>
        </td>
      </tr>    
      <% } %>
    </tbody>
  </table>
</div>
