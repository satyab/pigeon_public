<div class="container">
  <table class="table table-hover clickable-table">
    <thead>
      <tr>
        <th>#</th>
        <th>Name</th>
        <th>Description</th>            
      </tr>
    </thead>
    <tbody>
      <% for(i in apps ) {%>
      <tr >
        <td>
          <a href="/app/show/<%=apps[i].id%>">
            <%=apps[i].id%>
          </a>
        </td>
        <td>
          <a href="/app/show/<%=apps[i].id%>">            
          <%=apps[i].app_name%>
          </a>
        </td>
        <td>
          <a href="/app/show/<%=apps[i].id%>">            
          <%=apps[i].app_info%>
          </a>
        </td>      
      </tr>    
      <% } %>
    </tbody>
  </table>
</div>
