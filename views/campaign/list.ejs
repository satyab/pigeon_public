<div class="container table-responsive">
  <table class="table table-hover clickable-table">
    <thead>
      <tr>
        <th>Name</th>
        <th>Impressions</th>            
      </tr>
    </thead>
    <tbody>
      <% for(i in campaigns ) {%>
      <tr >
        <td>
          <a href="/campaign/show/<%=campaigns[i].id%>">
            <%=campaigns[i].name%>
          </a>
        </td>
        <td>
          <a href="/campaign/show/<%=campaigns[i].id%>">            
          <%=campaigns[i].limit - campaigns[i].remainingLimit%>
          </a>
        </td>
      </tr>    
      <% } %>
    </tbody>
  </table>
</div>
